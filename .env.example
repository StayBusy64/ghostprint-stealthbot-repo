# Target channel's video feed (used by ghostbot.js or subbot.js)
CHANNEL_URL=https://www.youtube.com/@StayBusy64/videos

# How many videos to engage with per run
MAX_VIDEOS=5

# Watch time per video in seconds (random between MIN/MAX)
WATCH_TIME_MIN=45
WATCH_TIME_MAX=300

# Discord webhook to get live pings from bots
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_id

# Google account creds (used by subbot.js for login + sub)
GOOGLE_EMAIL=your_email@gmail.com
GOOGLE_PASSWORD=your_google_pass
